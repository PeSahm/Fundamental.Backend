{
  "ConnectionStrings": {
    "FundamentalDbContext": ""
  },
  "Redis": {
    "ConnectionString": "",
    "User": "",
    "Password": "",
    "DefaultDatabase": 0,
    "ConnectTimeout": 5000,
    "ConnectRetry": 3,
    "KeepAlive": 60,
    "AbortOnConnectFail": false,
    "ConfigCheckSeconds": 60
  },
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      }
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "UltraTone": "Error",
        "Microsoft.AspNetCore": "Error",
        "Microsoft.Hosting.Lifetime": "Error",
        "Microsoft.AspNetCore.SignalR": "Error",
        "Microsoft.AspNetCore.Http.Connections": "Error",
        "Microsoft.EntityFrameworkCore": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Level:u3} [{Timestamp:u}] [{RequestId}] {SourceContext}{NewLine}    {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://fundamental-seq"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithExceptionDetails"
    ]
  },
  "CorsOrigins": [
    "http://localhost:5000",
    "https://*.stockscreeners.ir",
    "https://localhost:4200"
  ],
  "Server": "http://localhost:5000",
  "Mdp": {
    "Url": "https://mdpapi.pikadbazar.ir/",
    "Statement": "api/v2/Statement",
    "StatementJson": "api/v1/StatementJson",
    "Publishers": "api/v1/StatementJson/publishers",
    "ShareHolders": "/api/AutoQuery/SymbolShareholders",
    "Symbol": "/api/AutoQuery/Symbols",
    "TradeHistory": "/api/AutoQuery/TradeHistory",
    "Index": "/api/v1/Index"
  },
  "TseTmc": {
    "Url": "https://cdn.tsetmc.com/",
    "IndexCompany": "/api/ClosingPrice/GetIndexCompany",
    "ClosingPriceInfo" : "/api/ClosingPrice/GetClosingPriceInfo/"
  },
  "JobEnabled": "false",
  "Coravel": {
    "Pro": {
      "UseClientTimezone": "true"
    }
  }
}
